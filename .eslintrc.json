{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": 0,
        "no-tabs": 0,
        "camelcase": 0,
        "no-trailing-spaces": 0,
        "arrow-parens": 0,
        "indent": 0
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-prettier"
    ],
    "parser": "@typescript-eslint/parser"
}